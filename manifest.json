{
    "name": "RR Timer Barebones",
    "version": "0.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons":{"48": "rose48.png"},
    "browser_action": {
        "default_icon": "rose-off.png"
    },
    "permissions": [
        "activeTab",
        "<all_urls>",
        "http://*/*",
        "storage",
        "contextMenus"
    ],
    "options_ui": {
      "page": "options.html",
      "chrome_style": true
    }
}
